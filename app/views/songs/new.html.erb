<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
    <%= f.label :artist_name, "Artist Name:" %>
  <%= f.text_field :artist_name %>
  <br>
  <datalist id="genre_id">
    <% Genre.all.each do |genre| %>
    <option value="<%= genre.name %>" >
      <%end%>
    </datalist>
  <br>

  <%= f.submit %>
<% end %>
