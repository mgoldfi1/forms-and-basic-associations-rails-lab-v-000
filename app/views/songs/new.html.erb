<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
    <%= f.label :artist_name, "Artist Name:" %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.collection_check_boxes(:song, :genre_id, Genre.all, :id)%>
  <br>

  <%= f.submit %>
<% end %>
